    "mdi-react": "^7.3.0",
    "normalize.css": "^8.0.1",
    "qrcode": "^1.4.4",
    "raf": "^3.4.1",
    "react": "^17.0.1",
    "react-cool-onclickoutside": "^1.5.5",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^17.0.1",
    "react-helmet-async": "^1.0.7",
    "react-image": "^4.0.3",
